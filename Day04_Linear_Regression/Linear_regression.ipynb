import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
df = pd.read_csv("placement.csv")

X = df["cgpa"].values
Y = df["package"].values
plt.scatter(X, Y)
plt.xlabel("CGPA")
plt.ylabel("Package")
plt.title("CGPA vs Package")
plt.show()
x_mean = np.mean(X)
y_mean = np.mean(Y)

numerator = np.sum((X - x_mean) * (Y - y_mean))
denominator = np.sum((X - x_mean) ** 2)

m = numerator / denominator
c = y_mean - m * x_mean

print("Slope (m):", m)
print("Intercept (c):", c)
Y_pred = m * X + c

plt.scatter(X, Y)
plt.plot(X, Y_pred)
plt.xlabel("CGPA")
plt.ylabel("Package")
plt.title("Linear Regression with line")
plt.show()

